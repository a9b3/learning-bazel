{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "jsscripts/*": ["*"]
    },
    "module": "commonjs",
    "noImplicitAny": false,
    "preserveConstEnums": true,
    "removeComments": true,
    "target": "es6",
    "sourceMap": true,
    "skipLibCheck": true,
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "jsx": "react",
    "lib": ["es6", "dom"]
  },
  "exclude": ["bazel-*", "node_modules"]
}
